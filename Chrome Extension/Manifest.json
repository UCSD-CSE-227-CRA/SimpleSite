{
  "name":"Cookie with Secret",
  "version":"1.0",
  "description":"Secure your login credentials",
  "permissions":[
    "webRequest",
    "webRequestBlocking",
    "cookies",
    "http://localhost/"
  ],
  "background":{
    "scripts":[
      "config.js",
      "background.js"
    ]
  },
  "browser_action":{
    "default_title":"Cookie with Secret",
    "default_icon":"images/lock_black.png"
  },
  "icons":{"128":"images/lock_green.png"},
  "manifest_version":2
}